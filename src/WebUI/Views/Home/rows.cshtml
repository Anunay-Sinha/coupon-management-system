@model IEnumerable<Coupon>

@foreach (var o in Model)
{
    <li id='o@(o.Id)' class="meal">
        <div>@o.Name
        </div>
        <div>
            <div class="fl"><img  src='@Url.Content("~/pictures/Coupons/" + (o.HasPic ? o.Id : 0) + ".jpg")' class="thumb"  alt='picture' /></div>
            <div class="comments">@o.Comments</div>
            <br class="cbt"/>
        </div>      
    </li>
}